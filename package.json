{
    "name": "my-vite-protobuf-benchmark",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "generate:proto": "npx pbjs -t static-module -w es6 -o src/MyModel.js src/MyModel.proto && npx pbts -o src/MyModel.d.ts src/MyModel.js",
        "prepare": "husky",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
    },
    "dependencies": {
        "ag-charts-enterprise": "^11.2.1",
        "ag-grid-community": "^33.2.1",
        "ag-grid-enterprise": "^33.2.1",
        "ag-grid-react": "^33.2.1",
        "pbf": "^4.0.1",
        "protobufjs": "^7.4.0",
        "protobufjs-cli": "^1.1.3",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.21.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^15.15.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "prettier": "^3.5.3",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.24.1",
        "vite": "^6.2.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    }
}
